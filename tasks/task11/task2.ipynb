     ]
    }
   ],
   "source": [
    "# Regular function\n",
    "def add(x, y):\n",
    "    return x + y\n",
    "\n",
    "# Equivalent lambda function\n",
    "add_lambda = lambda x, y: x + y\n",
    "\n",
    "# Using both functions\n",
    "result1 = add(3, 5)\n",
    "result2 = add_lambda(3, 5)\n",
    "\n",
    "print(result1)  \n",
    "print(result2)  "
   ]
